pkgname=gpe-icons
pkgver=0.25
pkgrel=0
pkgdesc="Default icon theme for the GPE Palmtop Environment."
url="https://wiki.postmarketos.org/wiki/GPE"
arch="noarch"
license="GPL-2.0 LGPL-2.0"
source="http://http.debian.net/debian/pool/main/g/${pkgname}/${pkgname}_${pkgver}.orig.tar.gz"
builddir="${srcdir}/${pkgname}-${pkgver}"
options="!check"
package() {
	cd "$builddir"
	make PREFIX=/usr DESTDIR="$pkgdir" install-program
}
